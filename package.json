{
  "name": "React Single Page app with Server side rendering",
  "homepage": "https://cra-ssr.herokuapp.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "connected-react-router": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "forcedomain": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-frontload": "^1.0.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "serve": "cross-env NODE_ENV=production node ./server/index.js",
    "build:serve": "npm run build && npm run serve"
  },
  "devDependencies": {
    "cross-env": "^5.2.0"
  }
}
